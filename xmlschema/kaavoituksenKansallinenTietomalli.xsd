						<xs:annotation>
							<xs:documentation>Kaavan osa-alueen tyyppi</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="obligatory" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Tieto kaavan osa-alueen sitovuudesta</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="restrictiveFeature" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">

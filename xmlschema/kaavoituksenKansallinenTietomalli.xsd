						<xs:annotation>
							<xs:documentation>Kaavan tarkennemerkinnän tyyppi</xs:documentation>
						</xs:annotation>
					</xs:element>					
					<xs:element name="numericValue" type="kt:NumericValuePropertyType" minOccurs="0" maxOccurs="unbounded">

	<xs:complexType name="RegulativeTextPropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="kt:RegulativeText"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Kaavan numeerinen merkintä -->
	<xs:element name="NumericValue" type="kt:NumericValueType" substitutionGroup="gml:AbstractGML">
		<xs:annotation>
			<xs:documentation>Kaikki kaavan numeerinen tieto tulisi tallentaa luokiteltuna kaavan numeerisena merkintänä</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="NumericValueType">
		<xs:complexContent>
			<xs:extension base="gml:AbstractGMLType">
				<xs:sequence>
					<xs:element ref="kt:localizedDescription" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Vapaamuotoinen kuvaus kaavan numeerisesta merkinnästä</xs:documentation>

					</xs:element>
					<xs:element ref="kt:localizedName" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Tapahtuman nimi / kuvaus</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="process" type="gml:ReferenceType" minOccurs="0">

						<xs:annotation>
							<xs:documentation>Kaavan taustatiedon geometria</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="kt:localizedName" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Kaavan taustatiedon nimi / vapaamuotoinen kuvaus</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="type" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishInformativeFeatureType" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Kaavan taustatiedon tyyppi</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InformativeFeaturePropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="kt:InformativeFeature"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Kaavadokumenttien abstrakti kuvaus -->
	<xs:element name="AbstractReferencedResource" type="kt:AbstractReferencedResourceType" abstract="true" substitutionGroup="gml:AbstractGML">
		<xs:annotation>
			<xs:documentation>Absrakti kuvaus GML-kohteesta, joka määrittelee kaavan viiteaineistoa</xs:documentation>
		</xs:annotation>
	</xs:element>

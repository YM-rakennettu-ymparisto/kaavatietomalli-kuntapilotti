<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:gmlsf="http://www.opengis.net/gmlsf/2.0" xmlns:kt="http://louhi.sitowise.com/Kuntapilotti_2019/" targetNamespace="http://louhi.sitowise.com/Kuntapilotti_2019/" elementFormDefault="qualified" version="1.1.5">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/xml.xsd"/>
	<xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="http://schemas.opengis.net/gml/3.2.1/gml.xsd"/>
	<xs:import namespace="http://www.opengis.net/gmlsf/2.0" schemaLocation="http://schemas.opengis.net/gmlsfProfile/2.0/gmlsfLevels.xsd"/>
	<xs:annotation>
		<xs:appinfo source="http://schemas.opengis.net/gmlsfProfile/2.0/gmlsfLevels.xsd">
			<gmlsf:ComplianceLevel>1</gmlsf:ComplianceLevel>
		</xs:appinfo>
	</xs:annotation>

	<!-- Abstrakti juurielementti -->
	<xs:element name="SpatialPlanElementSet" type="kt:SpatialPlanElementSetType" substitutionGroup="gml:AbstractGML">
		<xs:annotation>
			<xs:documentation>Abstraktin tason kohde kaavakokonaisuuden tallentamiseen GML-skeemassa</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="SpatialPlanElementSetType">
		<xs:complexContent>
			<xs:extension base="gml:AbstractFeatureType">
				<xs:sequence minOccurs="0" maxOccurs="unbounded">
					<xs:element name="featureMember">
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="gml:AbstractFeatureMemberType">
									<xs:sequence>
										<xs:element ref="gml:AbstractFeature"/>
									</xs:sequence>
								</xs:extension>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="AbstractGMLWithMandatoryIdentifierType">
		<xs:complexContent>
			<xs:restriction base="gml:AbstractGMLType">
				<xs:sequence>
					<xs:element ref="gml:metaDataProperty" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Metatiedot, joilla yksilöidään ja kuvataan GML-kohde</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:description" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Vapaaehtoinen kuvaus GML-kohteesta</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:descriptionReference" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Vapaaehtoinen viittaus kohteen tarkempaan kuvaukseen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:identifier" minOccurs="1">
						<xs:annotation>
							<xs:documentation>GML-kohteen yksilöivä tunniste, jokaisella aineistoversiolla erillinen tunniste kohteelle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:name" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Vapaaehtoinen nimitys kohteelle</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute ref="gml:id" use="required">
					<xs:annotation>
						<xs:documentation>Aineiston sisäisesti kohteen yksilöivä tunniste, jota käytetään viittauksissa</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="AbstractFeatureWithMandatoryIdentifierType">
		<xs:complexContent>
			<xs:restriction base="gml:AbstractFeatureType">
				<xs:sequence>
					<xs:element ref="gml:metaDataProperty" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Metatiedot, joilla yksilöidään ja kuvataan GML-kohde</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:description" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Vapaaehtoinen kuvaus GML-kohteesta</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:descriptionReference" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Vapaaehtoinen viittaus kohteen tarkempaan kuvaukseen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:identifier" minOccurs="1">
						<xs:annotation>
							<xs:documentation>GML-kohteen yksilöivä tunniste, jokaisella aineistoversiolla erillinen tunniste kohteelle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:name" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Vapaaehtoinen nimitys kohteelle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:boundedBy" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Vapaaehtoinen rajaus</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="gml:location" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Vapaaehtoinen sijainti kohteelle</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute ref="gml:id" use="required">
					<xs:annotation>
						<xs:documentation>Aineiston sisäisesti kohteen yksilöivä tunniste, jota käytetään viittauksissa</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	
	<!-- Kaavatietojen abstrakti kuvaus -->
	<xs:element name="AbstractPlanningObject" type="kt:AbstractPlanningObjectType" abstract="true" substitutionGroup="gml:AbstractFeature">
		<xs:annotation>
			<xs:documentation>Abstraktin tason kohde, joka tallentaa yksittäisen kaavakohteen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="AbstractPlanningObjectType" abstract="true">
		<xs:complexContent>
			<xs:extension base="kt:AbstractFeatureWithMandatoryIdentifierType">
				<xs:sequence>
					<xs:element name="created" type="xs:dateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Vapaaehtoinen luontiajankohta kaavan GML-kohteelle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- Kaavatunnus, joka säilyy versiosta toiseen -->					
					<xs:element name="planningObjectIdentifier" type="xs:anyURI">
						<xs:annotation>
							<xs:documentation>Uniikki, kaavan tai muun yksittäisen kaavakohteen yksilöivä tunniste. Aineiston muuttuessa tunnusta ei vaihdeta, jos vastaavuus samaan kohteeseen. Ilmaistaan INSPIRE-yhteensopivassa muodossa, yksilöivä osuus UUID-muodossa</xs:documentation>
						</xs:annotation>
						<!-- Pilotissa tunnisteena UUID, mutta jatkossa JHS-suosituksen mukainen tunniste. -->
					</xs:element>
					<xs:element name="obsoletes" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:AbstractPlanningObject</gml:targetElement>
							</xs:appinfo>
							<xs:documentation>Viittaus kohteeseen, joka korvaa viittaavan kohteen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="obsoletedBy" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:AbstractPlanningObject</gml:targetElement>
							</xs:appinfo>
							<xs:documentation>Viittaus kohteeseen, jonka viittaava (uusi kohde) korvaa</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="informativeDocumentation" type="kt:ReferencedDocumentPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Viittaus kohteeseen liittyvään dokumenttiin (aineistoviittaus)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="planDescription" type="kt:PlanDescriptionPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Viittaus kaavaselostukseen / selostuksen osaan (PlanDescription), joka liittyy viittavaan kohteeseen</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractPlanningObjectPropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="kt:AbstractPlanningObject"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:element name="localizedName" type="kt:LocalizedNameType">
		<xs:annotation>
			<xs:documentation>Kieliversioitu nimi</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="LocalizedNameType">
		<xs:simpleContent>
			<xs:extension base="gml:CodeType">
				<xs:attribute ref="xml:lang" use="optional">
					<xs:annotation>
						<xs:documentation>Kielen määrittely</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	
	<xs:element name="localizedObjective" type="kt:LocalizedObjectiveType">
		<xs:annotation>
			<xs:documentation>Kieliversioitu kuvaus kaavan tavoitteesta</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="LocalizedObjectiveType">
		<xs:simpleContent>
			<xs:extension base="gml:CodeType">
				<xs:attribute ref="xml:lang" use="optional">
					<xs:annotation>
						<xs:documentation>Kielen määrittely</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:element name="localizedDescription" type="kt:LocalizedDescriptionType">
		<xs:annotation>
			<xs:documentation>Kieliversioitu kohteen kuvaus</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="LocalizedDescriptionType">
		<xs:simpleContent>
			<xs:extension base="gml:StringOrRefType">
				<xs:attribute ref="xml:lang" use="optional">
					<xs:annotation>
						<xs:documentation>Kielen määrittely</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	
	<!-- Kaava -->
	<xs:element name="SpatialPlan" type="kt:SpatialPlanType" substitutionGroup="kt:AbstractPlanningObject">
		<xs:annotation>
			<xs:documentation>Kaava määrittää kaavoitushankkeen aikaiset tiedot ja toimii linkkinä kohdetietojen ja selvitysaineiston välillä. Kaavarajaus voi olla hyvin suuntaa-antava tai tarkasti kohdistettu riippuen kaavoituksen vaiheesta.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="SpatialPlanType">
		<xs:complexContent>
			<xs:extension base="kt:AbstractPlanningObjectType">
				<xs:sequence>
					<xs:element name="contact" type="xs:string" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Kaavan yhteyshenkilö</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="extent" type="gml:GeometryPropertyType">
						<xs:annotation>
							<xs:documentation>Kaavan ulkoraja, voi muodostua useammasta alueesta</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="landAdminstrationAuthority" type="xs:anyURI">
						<xs:annotation>
							<xs:documentation>Kaavan vastuutaho (kunta)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="language" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishLanguage" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Kaavan laadintakieli, joka ilmaistaan koodilistan mukaisella arvolla</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="kt:localizedObjective" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Kaavan tavoite</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="kt:localizedName" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Kaavan nimi</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="planningLevel" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishDocumentRole" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Kaavataso</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="planType" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishSpatialPlanType" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Kaavan tyyppi</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="status" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishSpatialPlanStatus" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Kaavakohteen vaihe</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="origin" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishSpatialPlanOrigin" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Alkuperä eli tieto tavasta, jolla alkuperäinen kaava on tuotettu</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="validFrom" type="xs:date" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Ajankohta, jolloin kyseinen kohde on tullut / tulee voimaan</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="validTo" type="xs:date" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Ajankohta, jolloin kohteen voimassaolo on päättynyt / päättyy</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="conformedBy" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:SpatialPlan</gml:targetElement>
							</xs:appinfo>
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Viittaus kaavakohteeseen (SpatialPlan), joka korvaa tämän kohteen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="conformsTo" type="gml:ReferenceType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:SpatialPlan</gml:targetElement>
							</xs:appinfo>
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Viittaus kaavakohteeseen (SpatialPlan), jonka tämä kohde korvaa (käytännössä kaavaprosessin etenemisen myötä päivitetty kohde)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="zoningElement" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:ZoningElement</gml:targetElement>
							</xs:appinfo>
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Viittaus maankäyttöalueeseen (ZoningElement), joka liittyy kaavaan</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="regulativeNote" type="kt:RegulativeTextPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Viittaus kaavamääräykseen (RegulativeText), joka koskee koko kaavan aluetta</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="affectingIncludedFeature" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:InformativeFeature</gml:targetElement>
							</xs:appinfo>
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Viittaus kaavan taustatietoon (InformativeFeature), joka on paikkatietokohde</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="backgroundData" type="kt:ReferencedDataPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Aineistoviittaus (ReferencedData) kaavaan liittyvään taustatietoon, jota ei tuoda paikkatietomuodossa</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SpatialPlanPropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="kt:SpatialPlan"/>
		</xs:sequence>
	</xs:complexType>
			
	<!-- Maankäyttöalue -->
	<xs:element name="ZoningElement" type="kt:ZoningElementType" substitutionGroup="kt:AbstractPlanningObject">
		<xs:annotation>
			<xs:documentation>Maankäyttöalue, joka on osoitettu kaavassa tietyyn käyttötarkoitukseen (yksi tai useampi). Jos tontit ovat jo olemassa tai ne luodaan kaavoituksen yhteydessä, ne ovat maankäyttöalueita, jotka linkitetään käyttötarkoitustietoon. Muussa tapauksessa käytetään isompia alueita tarpeen mukaan.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ZoningElementType">
		<xs:complexContent>
			<xs:extension base="kt:AbstractPlanningObjectType">
				<xs:sequence>
					<xs:element name="geometry" type="gml:GeometryPropertyType">
						<xs:annotation>
							<xs:documentation>Maankäyttöalueen geometria, tulee olla aluemuotoinen, mutta multigeometria sallitaan</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="kt:localizedName" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Maankäyttöalueen nimi, vapaamuotoinen teksti / numero. Kentässä voidaan tallentaa esim. kadun nimi, asuinalueen korttelinumero tai tontin numero (pelkkä tontti tai pidempi numerosarja)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="type" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishZoningElementType" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Maankäyttöalueen tyyppi</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="upToDateness" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishUpToDateness" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Maankäyttöalueen ajanmukaisuus</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="plannedSpace" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:PlannedSpace</gml:targetElement>
							</xs:appinfo>
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Viittaus kaavan osa-alueeseen (PlannedSpace), joka liittyy viittaavaan maankäyttöalueeseen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="planningDetail" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:PlanningDetail</gml:targetElement>
							</xs:appinfo>
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Viittaus kaavan tarkennemerkintään (PlanningDetail), joka liittyy viittaavaan maankäyttöalueeseen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="regulativeNote" type="kt:RegulativeTextPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Viittaus kaavamääräykseen (RegulativeText), joka liittyy viittaavaan maankäyttöalueeseen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="affectingIncludedFeature" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:InformativeFeature</gml:targetElement>
							</xs:appinfo>
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Viittaus paikkatietomuotoiseen kaavan taustatietoon (InformativeFeature), joka liittyy viittaavaan maankäyttöalueeseen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="subdivision" type="gml:ReferenceType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:appinfo>
								<gml:targetElement>kt:ZoningElement</gml:targetElement>
							</xs:appinfo>
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Viittaus toiseen maankäyttöalueeseen (ZoningElement), joka liittyy viittaavaan maankäyttöalueeseen. Asemakaavassa maankäyttöalueiden sisäkkäisyys sallitaan ohjeellisen tontin kohdalla</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="landUse" type="kt:PlannedLandUsePropertyType" minOccurs="0" maxOccurs="unbounded">	
						<xs:annotation>
							<xs:documentation>Viittaus alueen käyttötarkoitukseen (PlannedLandUse), joka määrittää maankäyttöaluetta</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="numericValue" type="kt:NumericValuePropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Viittaus kaavan numeeriseen merkintään (NumericValue), joka määrittää maankäyttöaluetta, esimerkiksi tehokkuusluku tai autopaikkojen määrä</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="affectingReferencedData" type="kt:ReferencedDataPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>

	<!-- Alueen käyttötarkoitus -->
	<xs:element name="PlannedLandUse" type="kt:PlannedLandUseType" substitutionGroup="gml:AbstractGML">
		<xs:annotation>
			<xs:documentation>Alueen käyttötarkoituksen kuvaus. Kohde kuvaa sekä käyttötarkoituksen lajin että sen ulottuvuuden ja se on aina linkitettynä maankäyttöalueeseen</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="PlannedLandUseType">
		<xs:complexContent>
			<xs:extension base="gml:AbstractGMLType">
				<xs:sequence>
					<xs:element name="fractionOfTotalSpace" type="xs:double" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Alueen käyttötarkoituksen koko, ilmaistaan osuus viitatusta maankäyttöalueesta desimaaliluvulla</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="landUseKind" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishLandUseKind" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Alueen käyttötarkoitus koodilistan avulla ilmaistuna</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="previousLandUseKind" type="gml:ReferenceType" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishLandUseKind" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Alueen aiempi käyttötarkoitus</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="verticallyConstrainedAbove" type="gml:MeasureType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Alueen käyttötarkoituksen rajoitus alhaalta katsottuna, esimerkiksi korkeustieto, jonka alle käyttötarkoitus rajautuu</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="verticallyConstrainedBelow" type="gml:MeasureType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Alueen käyttötarkoituksen rajoitus ylhäältä katsottuna, esimerkiksi korkeustieto, jonka yläpuolelle käyttötarkoitus rajautuu</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PlannedLandUsePropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="kt:PlannedLandUse"/>
		</xs:sequence>
	</xs:complexType>
	

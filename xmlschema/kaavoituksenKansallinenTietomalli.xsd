						<xs:annotation>
							<xs:documentation>Kaavan numeerisen merkinnän nimi, koodilistan mukainen tyyppi</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="value" type="gml:MeasureType">
						<xs:annotation>
							<xs:documentation>Kaavan numeerisen merkinnän arvo</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NumericValuePropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="kt:NumericValue"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Kaavan käsittelytieto -->
	<xs:element name="OrdinanceProcessEvent" type="kt:OrdinanceProcessEventType" substitutionGroup="gml:AbstractFeature">
		<xs:annotation>
			<xs:documentation>Yksityiskohtaiset tiedot tapahtumasta, joka liittyy kaavaprosessiin</xs:documentation>
		</xs:annotation>
	</xs:element>

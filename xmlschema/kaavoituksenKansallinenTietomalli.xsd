							<xs:documentation>Viittauspäivämäärä</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reference" type="xs:anyURI">
						<xs:annotation>
							<xs:documentation>Viittaus ulkoiseen lähteeseen (URI)</xs:documentation>

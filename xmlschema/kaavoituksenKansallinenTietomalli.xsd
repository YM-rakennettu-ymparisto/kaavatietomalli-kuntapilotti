	<!-- Käyttötarkoitus -->
	<xs:element name="PlannedLandUse" type="kt:PlannedLandUseType" substitutionGroup="gml:AbstractGML">
		<xs:annotation>
			<xs:documentation>The description of land use kind. This object has the information of the type and dimension and it is always linked to some zoning element of the plan</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="PlannedLandUseType">
		<xs:complexContent>

							<xs:documentation>Kaavan tarkennemerkintään liittyvä kaavan numeerinen merkintä</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="affectingReferencedData" type="kt:ReferencedDataPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>

	<xs:element name="ReferencedDocument" type="kt:ReferencedDocumentType" substitutionGroup="kt:AbstractReferencedResource">
		<xs:annotation>
			<xs:documentation>Asiakirja, joka liittyy kiinteästi kaavaan ja luodaan kaavoituksen aikana</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ReferencedDocumentType">
		<xs:complexContent>
			<xs:extension base="kt:AbstractReferencedResourceType">
				<xs:sequence>
					<xs:element name="language" type="gml:ReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishLanguage" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Asiakirjan kieli</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="role" type="gml:ReferenceType" minOccurs="0">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishDocumentRole" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Kaavan liitteen rooli</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="type" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishDocumentType" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Kaavan liitteen tyyppi</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>

					<xs:element name="regulativeNote" type="kt:RegulativeTextPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Viittaus kaavan osa-alueeseen liittyvään kaavamääräykseen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="numericValue" type="kt:NumericValuePropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Viittaus kaavan osa-alueeseen liittyvään kaavan numeeriseen merkintään</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="affectingReferencedData" type="kt:ReferencedDataPropertyType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Kaavan aineistoviittaus (ReferencedData), joka liittyy viittaavaan kaavan osa-alueeseen</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PlannedSpacePropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="kt:PlannedSpace"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Kaavan tarkennemerkintä -->
	<xs:element name="PlanningDetail" type="kt:PlanningDetailType" substitutionGroup="kt:AbstractPlanningObject">

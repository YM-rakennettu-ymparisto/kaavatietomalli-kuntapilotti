	<xs:complexType name="RegulativeTextPropertyType">
		<xs:sequence minOccurs="0">
			<xs:element ref="kt:RegulativeText"/>
		</xs:sequence>
	</xs:complexType>
	
	<!-- Numeerinen merkintä -->
	<xs:element name="NumericValue" type="kt:NumericValueType" substitutionGroup="gml:AbstractGML">
		<xs:annotation>
			<xs:documentation>All numeric values of the plan should be saved as numeric value object with the code that describes the meaning of the value</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="NumericValueType">
		<xs:complexContent>

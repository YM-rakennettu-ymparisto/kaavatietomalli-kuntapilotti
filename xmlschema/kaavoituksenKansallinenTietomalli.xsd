							<xs:documentation>Kaavaselostuksen kuvaus, vapaamuotoinen teksti</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="type" type="gml:ReferenceType">
						<xs:annotation>
							<xs:appinfo source="http://www.luettelopalvelu.fi/codelist/FinnishPlanDescriptionType" />
						</xs:annotation>
						<xs:annotation>
							<xs:documentation>Kaavaselostuksen tyyppi, aihealue</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
